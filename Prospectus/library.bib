@article{Paljug1994,
abstract = {When multiple arms are used to manipulate a large object, it is$\backslash$nbeneficial and sometimes necessary to maintain and control contacts$\backslash$nbetween the object and the effector (the contacting surface of an arm)$\backslash$nthrough force closure. Rolling and/or sliding can occur at these$\backslash$ncontacts, and the system is, in general, characterized by holonomic as$\backslash$nwell as nonholonomic (including unilateral) constraints. In this paper,$\backslash$nthe control of planar rolling contacts is investigated. Multi-arm$\backslash$nmanipulation systems are typically redundant. In the authors' approach,$\backslash$na minimal set of inputs is employed to control the trajectory of the$\backslash$nsystem while the surplus inputs control the contact conditions. The$\backslash$ntrajectory includes the gross motion of the object as well as the$\backslash$nrolling motion at each contact. A nonlinear feedback scheme for$\backslash$nsimultaneous control of motion as well as contact conditions is$\backslash$npresented. A new algorithm which adapts a two-effector grasp with$\backslash$nrolling contacts to external loads and the trajectory is developed.$\backslash$nSimulations and experimental results are used to illustrate the salient$\backslash$nfeatures in control and planning},
annote = {Uses a tactile sensor at the hands to control the grip on the object for rolling contacts. Also uses a 6 dimensional force/torque sensor on one manipulator. Moves a soccer ball back and forth while keeping enough friction on the ball to not drop it.},
author = {Paljug, Eric and Yun, Xiaoping and Kumar, Vijay},
doi = {10.1109/70.313095},
file = {:home/kraudust/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Control of Rolling Contacts In Multi-Arm Manipulation.pdf:pdf},
isbn = {0-7803-0078-5},
issn = {1042296X},
journal = {IEEE Transactions on Robotics and Automation},
number = {4},
pages = {441--452},
title = {{Control of Rolling Contacts in Multi-Arm Manipulation}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=313095},
volume = {10},
year = {1994}
}
