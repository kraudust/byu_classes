@article{Kim2016,
abstract = {{\textless}p{\textgreater}The Tangent Bundle Rapidly Exploring Random Tree (TB-RRT) is an algorithm for planning robot motions on curved configuration space manifolds, in which the key idea is to construct random trees not on the manifold itself, but on tangent bundle approximations to the manifold. Curvature-based methods are developed for constructing tangent bundle approximations, and procedures for random node generation and bidirectional tree extension are developed that significantly reduce the number of projections to the manifold. Extensive numerical experiments for a wide range of planning problems demonstrate the computational advantages of the TB-RRT algorithm over existing constrained path planning algorithms.{\textless}/p{\textgreater}},
annote = {Goes over a path planning algorithm useful for kinematically constrained motions (i.e. using two arms to move an object through an environment with obstacles). They call it the tangent bundle RRT algorithm. Classical RRT algorithms are typically used for a flat space, whereas constrained motions are typically a curved space (i.e. classical RRT algorithms can't be used). This paper uses classical RRT algorithms on bounded tangent spaces to the constraint manifold, and projects the RRT to the constraint manifold when it violates a certain threshold.},
author = {Kim, Beobkyoon and Um, Terry Taewoong and Suh, Chansu and Park, F. C. and Berenson, D. and Srinivasa, S. and Kuffner, J. and HENDERSON, MICHAEL E. and S{\'{a}}nchez, Gildardo and Latombe, Jean-Claude and SENTIS, LUIS and KHATIB, OUSSAMA and Shvalb, N. and Shoham, M. and Liu, G. and Trinkle, J.C. and {Xinyu Tang} and Thomas, S. and Coleman, P. and Amato, N. M. and Yakey, J.H. and LaValle, S.M. and Kavraki, L.E. and Yamane, Katsu and Kuffner, James J. and Hodgins, Jessica K. and Park, Frank C. and Bobrow, James E.},
doi = {10.1017/S0263574714001234},
file = {:home/kraudust/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kim et al. - 2016 - Tangent bundle RRT A randomized algorithm for constrained motion planning.pdf:pdf},
issn = {0263-5747},
journal = {Robotica},
keywords = {Control of robotic systems,Motion planning,Path planning,Redundant manipulators,Service robots},
month = {jan},
number = {01},
pages = {202--225},
publisher = {Cambridge University Press},
title = {{Tangent bundle RRT: A randomized algorithm for constrained motion planning}},
url = {http://www.journals.cambridge.org/abstract{\_}S0263574714001234},
volume = {34},
year = {2016}
}
